{
    "name": "aixcoder",
    "displayName": "aixcoder",
    "description": "aixcoder code suggestion",
    "version": "0.0.2",
    "publisher": "nnthink",
    "repository": {
        "url": "https://github.com/felixhao28/vscode-plugin"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "aiXcoder configuration",
            "properties": {
                "aiXcoder.language": {
                    "type": "string",
                    "default": "java(Java)",
                    "description": "Default programming language, one of [\"java(Java)\", \"TensorFlow(Python)\"]."
                },
                "aiXcoder.endpoint": {
                    "type": "string",
                    "default": "https://dev.aixcoder.com/",
                    "description": "Specifies the endpoint for code suggestion server."
                },
                "aiXcoder.delay": {
                    "type": "integer",
                    "default": 3000,
                    "description": "The delay in milliseconds before sending requests."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.6",
        "@types/node": "^11.10.4",
        "eslint": "^5.15.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "mocha": "^6.0.2",
        "tslint": "^5.13.1",
        "typescript": "^3.3.3333",
        "vscode": "^1.1.30"
    },
    "dependencies": {
        "agentkeepalive": "^4.0.2",
        "request-promise": "^4.2.4"
    }
}
